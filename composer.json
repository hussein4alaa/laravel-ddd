{
    "name": "g4t/laravel-ddd",
    "description": "Generates domains for laravel Domain Driven Development",
    "keywords": [
        "DDD",
        "Laravel",
        "Domain Driven Development",
        "Domains",
        "Package"
    ],
    "minimum-stability": "stable",
    "homepage": "https://github.com/hussein4alaa/laravel-ddd",
    "authors": [
        {
            "name": "HusseinAlaa",
            "email": "hussein4alaa@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0 || ~5.0",
        "mockery/mockery": "^0.9.5"
    },
    "autoload": {
        "psr-4": {
            "g4t\\LaravelDDD\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "g4t\\LaravelDDD\\Tests\\": "tests/"
        }
    }
}
